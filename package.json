    "@aave/core-v3": "3.0.1-beta.1",
    "@aave/periphery-v3": "^3.0.1",
    "@aave/safety-module": "^1.0.3",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@openzeppelin/contracts": "^4.3.2",
    "@types/bluebird": "^3.5.36",
    "@types/chai": "^4.2.21",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.1",
    "bluebird": "^3.7.2",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "dotenv": "^10.0.0",
    "ethers": "^5.4.7",
    "hardhat": "^2.6.8",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-dependency-compiler": "^1.1.2",
    "hardhat-deploy": "^0.10.6",
    "jsondiffpatch": "^0.4.1",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.3.2"
  },
  "license": "AGPLv3",
  "scripts": {
    "run-env": "npm cßi && tail -f /dev/null",
    "cp-typechain": "cp typechain/*.d.ts ./dist/types/typechain",
    "build": "tsc --project lib.tsconfig.json && npm run cp-typechain",
    "clean": "rm -rf artifacts/ types/ dist/ typechain/",
    "compile": "SKIP_LOAD=true npx hardhat compile",
    "prepublish": "npm run clean && npm run compile && npm run build",
    "node": "npx hardhat node --no-deploy",
    "node:fork": "FORK=main npm run node --no-deploy",
    "deploy": "if [ \"$SKIP_COMPILE\" = 'true' ]; then echo '[Warning] Skipping compilation'; else npm run compile; fi && npx hardhat deploy --no-compile",
    "deploy:market:aave": "MARKET_NAME=Aave npm run deploy",
    "deploy:market:harmony": "MARKET_NAME=Harmony npm run deploy",
    "deploy:market:test": "MARKET_NAME=Test npm run deploy",
    "deploy:market:avalanche": "MARKET_NAME=Avalanche npm run deploy",
    "deploy:market:fantom": "MARKET_NAME=Fantom npm run deploy",
    "deploy:market:polygon": "MARKET_NAME=Polygon npm run deploy",
    "deploy:market:arbitrum": "MARKET_NAME=Arbitrum npm run deploy",
    "deploy:market:optimistic": "MARKET_NAME=Optimistic npm run deploy",
    "deploy:market:aave:fork": "FORK=main npm run deploy:market:aave",
    "deploy:market:aave:arbitrum-görli:fork": "FORK='arbitrum-görli' npm run deploy:market:arbitrum",
    "deploy:market:aave:arbitrum-test:fork": "FORK='arbitrum-testnet' npm run deploy:market:arbitrum",
    "deploy:market:aave:arbitrum:fork": "FORK=arbitrum npm run deploy:market:arbitrum",
    "deploy:market:aave:goerli:fork": "FORK='görli' npm run deploy:market:aave",
    "deploy:market:aave:optimism:fork": "FORK=optimism npm run deploy:market:optimistic",
    "deploy:market:avalanche:fork": "FORK=avalanche npm run deploy:market:avalanche",
    "deploy:market:polygon:fork": "FORK='polygon' npm run deploy:market:polygon",
    "test": "npm run compile && npm run test:local && npm run test:fork",
    "test:fork": "MARKET_NAME=Aave FORK=main npx hardhat test ./tests/__setup.spec.ts ./tests/fork/check-list.spec.ts",
    "test:local": "MARKET_NAME=Aave npx hardhat test ./tests/__setup.spec.ts ./tests/*.spec.ts"
  },
  "peerDependencies": {
    "@aave/core-v3": "1.x",
    "@aave/periphery-v3": "1.x",
    "hardhat": "^2.6.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/aave/aave-v3-deploy"
  },
  "dependencies": {
    "bip39": "^3.0.4",
    "defender-relay-client": "^1.11.1"
  }
}
